# Frontend environment variables for the Notes app

# Base URL for the backend API. Example: http://localhost:3001
REACT_APP_API_BASE=http://localhost:3001

# Optionally set if you prefer a different variable name (kept for compatibility)
REACT_APP_BACKEND_URL=

# Optional: URL of the frontend itself (for CORS docs/reference)
REACT_APP_FRONTEND_URL=http://localhost:3000

# Optional WebSocket URL if you add real-time features later
REACT_APP_WS_URL=

# Common CRA toggles
REACT_APP_NODE_ENV=development
REACT_APP_NEXT_TELEMETRY_DISABLED=1
REACT_APP_ENABLE_SOURCE_MAPS=true
REACT_APP_PORT=3000
REACT_APP_TRUST_PROXY=true
REACT_APP_LOG_LEVEL=info
REACT_APP_HEALTHCHECK_PATH=/health
REACT_APP_FEATURE_FLAGS=
REACT_APP_EXPERIMENTS_ENABLED=false
